import { IpfsClient, IpfsResult } from "./ipfsClient";
export declare class PinataIpfsClient extends IpfsClient {
    private pinataJwt;
    private dedicatedGatewayUrl?;
    constructor(pinataJwt: string | undefined, dedicatedGatewayUrl?: string);
    private getUrl;
    getSize(hashOrCid: string): Promise<IpfsResult>;
    read(hashOrCid: string): Promise<IpfsResult>;
    pin(data: string): Promise<IpfsResult>;
    unpin(hashOrCid: string): Promise<IpfsResult>;
}
