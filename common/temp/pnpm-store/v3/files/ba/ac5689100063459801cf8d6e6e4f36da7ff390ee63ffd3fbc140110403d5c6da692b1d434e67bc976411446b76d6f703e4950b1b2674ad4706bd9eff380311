import { Halo2LibWasm, Halo2Wasm } from "@axiom-crypto/halo2-wasm/web";
import { CircuitConfig } from "./types";
export declare const autoConfigCircuit: (config: CircuitConfig) => void;
export declare const setCircuit: (halo2wasm: Halo2Wasm, halo2lib: Halo2LibWasm, silent?: boolean) => void;
export declare function Halo2CircuitRunner(halo2wasm: Halo2Wasm, halo2lib: Halo2LibWasm, config: CircuitConfig, silent?: boolean): Readonly<{
    runFromString: (code: string, inputs: string) => Promise<{
        config: CircuitConfig;
    }>;
    run: <T extends {
        [key: string]: string | number | bigint;
    }>(f: (inputs: T) => Promise<void>, inputs: T) => Promise<void>;
}>;
